{
  "expo": {
    "name": "crave-search",
    "slug": "crave-search-mobile",
    "scheme": "crave",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "entryPoint": "./AppEntry.js",
    "assetBundlePatterns": ["**/*"],
    "plugins": [],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.brandonkimble.cravesearch",
      "deploymentTarget": "15.1",
      "infoPlist": {
        "NSLocalNetworkUsageDescription": "Allow $(PRODUCT_NAME) to connect to your local network for development"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.crave"
    },
    "extra": {
      "eas": {
        "projectId": "afbb04ee-213b-4227-a9d2-c87a8f413034"
      }
    },
    "owner": "brandonk"
  }
}
