{
  "uid": "integrations-overview",
  "title": "Crave Integrations & Queues",
  "tags": ["crave-search", "backend", "queues", "integrations"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "templating": {
    "list": [
      {
        "name": "queue",
        "label": "Queue",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "crave-prometheus" },
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "query": "label_values(bull_queue_jobs, queue)",
        "sort": 1,
        "current": { "selected": false, "text": "All", "value": ".*" }
      }
    ]
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "LLM Utilization (% of safe)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "crave-prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 80 },
              { "color": "red", "value": 95 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "calcs": ["lastNotNull"], "displayMode": "table" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "llm_rate_limiter_rpm_utilization_percent",
          "legendFormat": "RPM",
          "refId": "A"
        },
        {
          "expr": "llm_rate_limiter_tpm_utilization_percent",
          "legendFormat": "TPM",
          "refId": "B"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "LLM Reservation Wait (p95)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "crave-prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 2 },
              { "color": "red", "value": 10 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "calcs": ["lastNotNull"], "displayMode": "table" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(llm_reservation_wait_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "LLM Tokens (last-minute window)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "crave-prometheus" },
      "fieldConfig": {
        "defaults": { "unit": "short", "color": { "mode": "palette-classic" } },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "calcs": ["lastNotNull"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "llm_rate_limiter_tpm_current",
          "legendFormat": "currentTPM (used)",
          "refId": "A"
        },
        {
          "expr": "llm_rate_limiter_tpm_reserved",
          "legendFormat": "reservedTPM",
          "refId": "B"
        },
        {
          "expr": "llm_rate_limiter_tpm_window_tokens",
          "legendFormat": "windowTokens",
          "refId": "C"
        },
        {
          "expr": "llm_rate_limiter_tpm_safe",
          "legendFormat": "safeTPM",
          "refId": "D"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "LLM Throughput + Rate Limits",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "crave-prometheus" },
      "fieldConfig": {
        "defaults": { "unit": "req/s", "color": { "mode": "palette-classic" } },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "calcs": ["lastNotNull"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "sum(rate(llm_requests_total[5m])) by (outcome)",
          "legendFormat": "requests {{outcome}}",
          "refId": "A"
        },
        {
          "expr": "rate(llm_rate_limit_errors_total[5m])",
          "legendFormat": "rate_limit_errors",
          "refId": "B"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Bull Queue Backlog (waiting + delayed)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "crave-prometheus" },
      "fieldConfig": {
        "defaults": { "unit": "short", "color": { "mode": "palette-classic" } },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "calcs": ["lastNotNull"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "sum(bull_queue_jobs{status=~\"waiting|delayed\"}) by (queue)",
          "legendFormat": "{{queue}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Bull Active Jobs",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "crave-prometheus" },
      "fieldConfig": {
        "defaults": { "unit": "short", "color": { "mode": "palette-classic" } },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "calcs": ["lastNotNull"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "sum(bull_queue_jobs{status=\"active\"}) by (queue)",
          "legendFormat": "{{queue}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Bull Job Throughput",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "crave-prometheus" },
      "fieldConfig": {
        "defaults": { "unit": "jobs/s", "color": { "mode": "palette-classic" } },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "calcs": ["lastNotNull"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "sum(rate(bull_jobs_total{result=\"completed\"}[5m])) by (queue)",
          "legendFormat": "completed {{queue}}",
          "refId": "A"
        },
        {
          "expr": "sum(rate(bull_jobs_total{result=\"failed\"}[5m])) by (queue)",
          "legendFormat": "failed {{queue}}",
          "refId": "B"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Bull Job Processing Time (p95)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "crave-prometheus" },
      "fieldConfig": {
        "defaults": { "unit": "s", "color": { "mode": "palette-classic" } },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "calcs": ["lastNotNull"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(bull_job_processing_seconds_bucket{queue=~\"$queue\", result=\"completed\"}[5m])) by (le, queue, job_name))",
          "legendFormat": "{{queue}}/{{job_name}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Bull Job Wait Time (p95)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 32 },
      "datasource": { "type": "prometheus", "uid": "crave-prometheus" },
      "fieldConfig": {
        "defaults": { "unit": "s", "color": { "mode": "palette-classic" } },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "calcs": ["lastNotNull"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(bull_job_wait_seconds_bucket{queue=~\"$queue\"}[5m])) by (le, queue, job_name))",
          "legendFormat": "{{queue}}/{{job_name}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "External API Utilization (% of limit)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 32 },
      "datasource": { "type": "prometheus", "uid": "crave-prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 80 },
              { "color": "red", "value": 95 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "calcs": ["lastNotNull"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "max(external_api_rate_limit_utilization_percent{window=\"minute\"}) by (service, operation)",
          "legendFormat": "{{service}}/{{operation}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "External API Rate Limit Hits",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 40 },
      "datasource": { "type": "prometheus", "uid": "crave-prometheus" },
      "fieldConfig": {
        "defaults": { "unit": "req/s", "color": { "mode": "palette-classic" } },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "calcs": ["lastNotNull"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "sum(rate(external_api_rate_limit_hits_total[5m])) by (service, source)",
          "legendFormat": "{{service}} {{source}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Google Places Daily Utilization (% of limit)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 40 },
      "datasource": { "type": "prometheus", "uid": "crave-prometheus" },
      "fieldConfig": {
        "defaults": { "unit": "percent", "color": { "mode": "palette-classic" } },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "calcs": ["lastNotNull"] },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "max(external_api_rate_limit_utilization_percent{service=\"google-places\", window=\"day\"}) by (operation)",
          "legendFormat": "{{operation}}",
          "refId": "A"
        }
      ]
    }
  ]
}
